{
  "name": "wetube",
  "version": "1.0.0",
  "description": "The best way to watch videos.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doraemon0807/wetube-reloaded.git"
  },
  "scripts": {
    "start": "node build/init.js",
    "build": "npm run build:server && npm run build:assets",
    "build:server": "set NODE_ENV=production&&babel src -d build",
    "build:assets": "rimraf assets && webpack --mode=production --config webpack.config.js",
    "dev:server": "set NODE_ENV=development&&nodemon",
    "dev:assets": "rimraf assets && webpack --mode=development -w --config webpack.config.js"
  },
  "author": "doraemon0807",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doraemon0807/wetube-reloaded/issues"
  },
  "homepage": "https://github.com/doraemon0807/wetube-reloaded#readme",
  "dependencies": {
    "@ffmpeg/core": "^0.11.0",
    "@ffmpeg/ffmpeg": "^0.11.6",
    "aws-sdk": "^2.1261.0",
    "bcrypt": "^5.1.0",
    "connect-mongo": "^4.6.0",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.3",
    "mongoose": "^6.6.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.10.0",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.10"
  },
  "devDependencies": {
    "css-loader": "^6.7.1",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.20",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "babel-loader": "^8.2.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
